{
	"version" : "6.1.0",
	"general" : {
		"cleanup" : true,
		"working-directory" : "./Working",
		"keep-logs" : false,
		"always-suffix" : false,
		"max-kept-outputs" : 15,
		"logging-level" : "info",
		"force-serial-module-execution" : false,
		"use-computed-times" : true
	},
	"module" : {
		"grib" : {
			"enabled" : true,
			"class" : "toberumono.wrf.modules.GRIBModule"
		},
		"wps" : {
			"enabled" : true,
			"class" : "toberumono.wrf.modules.WPSModule",
			"namelist" : "namelist.wps",
			"dependencies" : [ "grib" ]
		},
		"wrf" : {
			"enabled" : true,
			"class" : "toberumono.wrf.modules.WRFModule",
			"namelist" : "run/namelist.input",
			"dependencies" : [ "wps" ]
		}
	},
	"path" : {
		"wrf" : "./WRFV3",
		"wps" : "./WPS"
	},
	"timing" : {
		"global" : {
			"duration" : {
				"enabled" : true,
				"days" : 0,
				"hours" : 3,
				"minutes" : 0,
				"seconds" : 0
			},
			"clear" : {
				"enabled" : true,
				"keep" : "hours"
			},
			"offset" : {
				"enabled" : true,
				"days" : 0,
				"hours" : 0,
				"minutes" : 0,
				"seconds" : 0
			},
			"round" : {
				"enabled" : [ "hours" ],
				"type" : "bucket",
				"arguments" : {
					"rounding-mode" : "floor",
					"hours-step" : 3,
					"hours-offset" : 0
				}
			}
		},
		"grib" : {
			"constant" : {
				"inherit" : true,
				"clear" : {
					"enabled" : true,
					"keep" : "hours"
				},
				"offset" : {
					"enabled" : true,
					"days" : 0,
					"hours" : 0,
					"minutes" : 0,
					"seconds" : 0
				},
				"round" : {
					"enabled" : [ "hours" ],
					"type" : "bucket",
					"arguments" : {
						"rounding-mode" : "floor",
						"hours-step" : 3,
						"hours-offset" : 0
					}
				}
			},
			"incremented" : {
				"inherit" : true,
				"clear" : {
					"enabled" : true,
					"keep" : "hours"
				},
				"offset" : {
					"enabled" : true,
					"days" : 0,
					"hours" : 0,
					"minutes" : 0,
					"seconds" : 0
				},
				"round" : {
					"enabled" : [ "hours" ],
					"type" : "bucket",
					"arguments" : {
						"rounding-mode" : "floor",
						"hours-step" : 3,
						"hours-offset" : 0
					}
				}
			}
		},
		"wps" : {
			"inherit" : true
		},
		"wrf" : {
			"inherit" : true
		}
	},
	"grib" : {
		"url" : "http://www.ftp.ncep.noaa.gov/data/nccf/com/nam/prod/nam.%tY%tm%td/nam.t00z.awip3d%iH.tm00.grib2",
		"max-concurrent-downloads" : 8,
		"timestep" : {
			"wrap" : false,
			"days" : 0,
			"hours" : 3,
			"minutes" : 0,
			"seconds" : 0
		}
	},
	"wps" : {
		"parallel" : {
			"is-dmpar" : false,
			"boot-lam" : false,
			"processors" : 2
		}
	},
	"wrf" : {
		"parallel" : {
			"is-dmpar" : true,
			"boot-lam" : false,
			"processors" : 2
		}
	}
}
